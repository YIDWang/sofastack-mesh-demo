<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

    <context id="MySql" defaultModelType="flat">
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"/>

        <commentGenerator>
            <property name="addRemarkComments" value="true"/>
            <property name="suppressDate" value="true"/>
        </commentGenerator>

        <jdbcConnection
                driverClass="com.mysql.cj.jdbc.Driver"
                connectionURL="jdbc:mysql://localhost:3306/test_sys_info"
                userId="root"
                password="123456"/>

        <javaModelGenerator targetPackage="com.alipay.sofa.ms.reply.entity" targetProject="src/main/java"/>
        <sqlMapGenerator targetPackage="mybatis.mapper" targetProject="src/main/resources/META-INF"/>

        <javaClientGenerator targetPackage="com.alipay.sofa.ms.reply.mapper" targetProject="src/main/java" type="XMLMAPPER"/>

        <table tableName="jdp_tb_trade" domainObjectName="JdpTbTradeDO">
            <generatedKey column="tid" sqlStatement="MySql" identity="true"/>
            <columnOverride column="created" javaType="java.util.Date" jdbcType="TIMESTAMP"/>
            <columnOverride column="modified" javaType="java.util.Date" jdbcType="TIMESTAMP"/>
            <columnOverride column="jdp_created" javaType="java.util.Date" jdbcType="TIMESTAMP"/>
            <columnOverride column="jdp_modified" javaType="java.util.Date" jdbcType="TIMESTAMP"/>
        </table>

    </context>
</generatorConfiguration>
